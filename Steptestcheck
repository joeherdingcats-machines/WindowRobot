IF FI_CHECKBIT = TRUE AND FO_STEPBIT1 = STEPBIT1_CONF AND FO_STEPBIT2 = STEPBIT2_CONF AND FO_STEPBIT3 = STEPBIT3_CONF AND FO_STEPBIT4 = STEPBIT4_CONF AND FO_STEPBIT5 = STEPBIT5_CONF AND FO_STEPBIT6 = STEPBIT6_CONF AND FO_STEPBIT7 = STEPBIT7_CONF AND FO_STEPBIT8 = STEPBIT8_CONF THEN
