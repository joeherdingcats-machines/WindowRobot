    StyleNumberCall := 36;
ELSIF JambMiterMill2 = TRUE THEN
    StyleNumberCall := 37;
ELSIF JambMiterGrind2 = TRUE THEN
    StyleNumberCall := 38;
ELSIF JambMiterMiniMill1 = TRUE THEN
    StyleNumberCall := 39;
ELSIF JambMiterMiniMill2 = TRUE THEN
    StyleNumberCall := 40;
ELSIF JambMiterMiniGrind1 = TRUE THEN
    StyleNumberCall := 41;
ELSIF JambMiterMiniGrind2 = TRUE THEN
    StyleNumberCall := 42;
ELSIF JambStraightMill1 = TRUE THEN
    StyleNumberCall := 43;
ELSIF JambStraightMill2 = TRUE THEN
    StyleNumberCall := 44;
ELSIF JambStraightMiniMill1 = TRUE THEN
    StyleNumberCall := 45;
ELSIF JambStraightMiniMill2 = TRUE THEN
    StyleNumberCall := 46;
ELSIF JambStraightMiniGrind1 = TRUE THEN
    StyleNumberCall := 47;
ELSIF JambStraightMiniGrind2 = TRUE THEN
    StyleNumberCall := 48;
ELSIF JambSexBolt1 = TRUE THEN
    StyleNumberCall := 49;
ELSIF JambSexBolt2 = TRUE THEN
    StyleNumberCall := 50;
ELSIF JambSexBolt3 = TRUE THEN
    StyleNumberCall := 51;
ELSIF JambSexBolt4 = TRUE THEN
    StyleNumberCall := 52;
ELSIF PROCESSTESTER = TRUE THEN
	StyleNumberCall := 53;
ELSE
	StyleNumberCall := 0;
END_IF;
