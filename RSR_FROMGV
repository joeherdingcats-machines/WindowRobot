IF MiterMill1 = TRUE THEN
    StyleNumberCall := 1;
ELSIF MiterMill2 = TRUE THEN
    StyleNumberCall := 2;
ELSIF MiterMillMini1 = TRUE THEN
    StyleNumberCall := 3;
ELSIF MiterMillMini2 = TRUE THEN
    StyleNumberCall := 4;
ELSIF MiterGrind1 = TRUE THEN
    StyleNumberCall := 5;
ELSIF MiterGrind2 = TRUE THEN
    StyleNumberCall := 6;
ELSIF MitergrindMini1 = TRUE THEN
    StyleNumberCall := 7;
ELSIF MitergrindMini2 = TRUE THEN
    StyleNumberCall := 8;
ELSIF LHInactiveMill1 = TRUE THEN
    StyleNumberCall := 9;
ELSIF LHInactiveMill2 = TRUE THEN
    StyleNumberCall := 10;
ELSIF LHInactiveMiniMill1 = TRUE THEN
    StyleNumberCall := 11;
ELSIF LHInactiveMiniMill2 = TRUE THEN
    StyleNumberCall := 12;
ELSIF RHInactiveMill1 = TRUE THEN
    StyleNumberCall := 13;
ELSIF RHInactiveMill2 = TRUE THEN
    StyleNumberCall := 14;
ELSIF RHInactiveMiniMill1 = TRUE THEN
    StyleNumberCall := 15;
ELSIF RHInactiveMiniMill2 = TRUE THEN
    StyleNumberCall := 16;
ELSIF LHHingeDrillSash = TRUE THEN
    StyleNumberCall := 17;
ELSIF RHHingeDrillSash = TRUE THEN
    StyleNumberCall := 18;
ELSIF LHHingeDrillJamb = TRUE THEN
    StyleNumberCall := 19;
ELSIF RHHingeDrillJamb = TRUE THEN
    StyleNumberCall := 20;
ELSIF HingeMillJamb1 = TRUE THEN
