ELSIF HingeMillJamb2 = TRUE THEN
    StyleNumberCall := 22;
ELSIF HingeMillJamb3 = TRUE THEN
    StyleNumberCall := 23;
ELSIF HingeMillJamb4 = TRUE THEN
    StyleNumberCall := 24;
ELSIF HandleDrill = TRUE THEN
    StyleNumberCall := 25;
ELSIF HandleMill = TRUE THEN
    StyleNumberCall := 26;
ELSIF TNutDrill1 = TRUE THEN
    StyleNumberCall := 27;
ELSIF TNutDrill2 = TRUE THEN
    StyleNumberCall := 28;
ELSIF TNutDrill3 = TRUE THEN
